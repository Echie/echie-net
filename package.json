{
    "name": "homepage-gatsby",
    "version": "1.0.0",
    "author": "Teemu Huovinen <teemu.huovinen@hotmail.fi>",
    "license": "MIT",
    "scripts": {
        "start": "gatsby build && gatsby serve -p $PORT",
        "dev": "gatsby develop --port 8080",
        "deploy": "gatsby build && cp CNAME public/CNAME && gh-pages -d public",
        "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.jsx'"
    },
    "dependencies": {
        "gatsby": "^2.4.2",
        "gatsby-background-image": "^0.5.3",
        "gatsby-image": "^2.0.41",
        "gatsby-plugin-react-helmet": "^3.0.12",
        "gatsby-plugin-sass": "^2.0.11",
        "gatsby-plugin-sharp": "^2.0.36",
        "gatsby-source-filesystem": "^2.0.33",
        "gatsby-transformer-sharp": "^2.1.19",
        "intersection-observer": "^0.6.0",
        "node-sass": "^4.12.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-helmet": "^5.2.1"
    },
    "devDependencies": {
        "gh-pages": "^2.0.1",
        "prettier": "^1.17.0",
        "sass-lint": "^1.13.1"
    }
}
